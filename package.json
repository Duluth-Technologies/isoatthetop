{
  "name": "isoatthetop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:fr": "ng serve -c fr",
    "build": "ng build",
    "build:pages": "ng build && node scripts/generate-gh-pages-root.mjs",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:map": "node scripts/generate-map.mjs && node scripts/create-final-map.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/localize": "^21.0.6",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "sharp": "^0.34.5",
    "staticmaps": "^1.13.1",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
